      "block_order": [
        "vendor",
        "title",
        "subtitle",
        "price",
        "variant_picker",
        "quantity_selector",
        "buy_buttons",
        "description",
        "share",
        "05f01fd1-b20c-4cf6-98cb-91a32f054072",
        "ee9ea5f9-4fea-4dc1-bcb0-1db3a102b742"
