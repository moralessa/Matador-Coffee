            "show_dynamic_checkout": true
          }
        },
        "description": {
          "type": "description",
          "settings": {}
        },
        "share": {
          "type": "share",
          "settings": {}
        },
        "05f01fd1-b20c-4cf6-98cb-91a32f054072": {
